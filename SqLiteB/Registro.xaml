<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="SqLiteB.Registro">
    <ContentPage.Content>
        <StackLayout>
            <Entry x:Name="txtNombre" Placeholder="Ingrese nombre"/>
            <Entry x:Name="txtUsuario" Placeholder="Ingrese usuario"/>
            <Entry x:Name="txtContrasena" Placeholder="Ingrese Contrasena" IsPassword="True"/>


            <Button x:Name="btnRegistro" Text="Registrar" Clicked="btnRegistro_Clicked"/>

        </StackLayout>
    </ContentPage.Content>
</ContentPage>